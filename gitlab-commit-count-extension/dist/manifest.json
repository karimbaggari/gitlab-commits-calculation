{
  "manifest_version": 3,
  "name": "GitLab Commit Analyzer",
  "version": "1.0",
  "description": "Analyze and visualize GitLab commits.",
  "action": {
    "default_popup": "index.html",
    "default_icon": "icon.png"
  },
  "permissions": ["activeTab", "http://localhost:8000/*"],
  "host_permissions": ["http://localhost:8000/*"]
}
